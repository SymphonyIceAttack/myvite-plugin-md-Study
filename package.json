{
  "name": "@symphonyiceattack/vite-plugin-template",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs.js",
  "browser": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "url": "https://github.com/SymphonyIceAttack/SymphonyIceAttackVitePluginTemplate"
  },
  "scripts": {
    "build": "rollup --config",
    "dev": "rollup --config -w",
    "test": "vitest",
    "watch": "vitest watch --coverage",
    "example:react": "cd examples/vite-react && yarn build",
    "prepublishOnly": "yarn run build",
    "release": "npx bumpp --push --tag --commit && yarn publish"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.24.3",
    "bumpp": "^8.2.1",
    "rollup": "^3.2.2",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^9.0.1",
    "@types/node": "^18.11.0",
    "loadsh": "^0.0.4",
    "lodash": "^4.17.21",
    "postcss": "^8.4.18",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.8"
  }
}